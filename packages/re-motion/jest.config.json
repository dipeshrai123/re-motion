{
  "preset": "ts-jest",
  "testEnvironment": "jsdom",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"],
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.ts"],
  "roots": ["src"],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx)$",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "moduleNameMapper": {
    "src/(.*)": "<rootDir>/src/$1"
  }
}
